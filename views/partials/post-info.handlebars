<article class="post my-3 mx-4">
    <div class="title-bar">
        <a href="/post/{{id}}"><h4>{{title}}</h4></a>
        <h6 class="side-title">Posted by {{user.username}} on {{format_date created_at}}</h6>
    </div>
    <div class="contents" >
        <p>{{contents}}</p>
    </div>
</article>


